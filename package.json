{
  "name": "lifetouch-cma-cli",
  "version": "1.0.0",
  "description": "户乐CMA项目脚手架",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "main": "./src/extension.js",
  "dependencies": {
    "@aws-sdk/util-utf8-node": "^0.1.0-preview.1",
    "@babel/core": "^7.10.3",
    "element-ui": "^2.4.5",
    "svgo": "^1.3.2",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.46.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0",
    "vue-cli-plugin-element": "~1.0.1"
  },
  "activationEvents": [
    "*"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "configuration": {
      "title": "cmaCli",
      "properties": {
        "cmaCli.svgDecorator.path": {
          "type": "string",
          "default": "/src/styles/svg",
          "format": "uri",
          "description": "Specifies the Svg files path relative to workspaceFolders."
        },
        "cmaCli.svgViewer.path": {
          "type": "string",
          "default": "/src/styles/svg",
          "format": "uri",
          "description": "Specifies the Svg files path relative to workspaceFolders."
        }
      }
    },
    "menus": {
      "explorer/context": [
        {
          "command": "js2json.transform",
          "when": "resourceLangId == javascript",
          "group": "7_modification"
        }
      ]
    },
    "commands": [
      {
        "command": "js2json.transform",
        "title": "CMA - Transform to Json"
      },
      {
        "command": "svgViwer.all",
        "title": "Preview all svg file and do something",
        "category": "CmaCli.SvgViewer"
      }
    ],
    "views": {
      "cma-cli": [
        {
          "id": "module",
          "name": "module"
        },
        {
          "id": "api",
          "name": "api"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "cma-cli",
          "title": "CMA-Cli",
          "icon": "src/media/hula.svg"
        }
      ]
    }
  },
  "displayName": "lifetouch-cma-cli",
  "engines": {
    "vscode": "^1.46.0"
  },
  "keywords": [
    "cmd",
    "lifetouch"
  ],
  "publisher": "yindongfang"
}
